set nocompatible
filetype off
set rtp+=~/.vim/bundle/vundle
call vundle#begin()
    Plugin 'VundleVim/Vundle.vim'
    Plugin 'jiangmiao/auto-pairs'
call vundle#end()
    filetype plugin indent on

syntax on
set nu
set relativenumber
set cursorline
set expandtab
set shiftwidth=4
set autoindent
set cindent
